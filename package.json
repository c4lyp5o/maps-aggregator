{
  "name": "maps-aggregator",
  "version": "1.0.0",
  "description": "A Google Maps API aggregator that accepts CSV or XLSX files and queries the Google Maps API for the latitude, longitude, and Google Maps URL of each address.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [
    "google-maps",
    "api",
    "aggregator",
    "geocoding",
    "csv",
    "xlsx"
  ],
  "author": "c4lyp5o",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "csv": "^6.3.6",
    "dotenv": "^16.4.4",
    "hyper-express": "^6.14.12",
    "joi": "^17.12.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}
